{
    "manifest_version": 2,
    "name": "React Content Script",
    "version": "0.1",
    "description": "React Content Script",
    "minimum_chrome_version": "22",
    "permissions": [
          "cookies",
          "*://github.com/*",
          "tabs",
          "webNavigation"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["*://github.com/*"],
        "css": ["build/static/css/main.57215138.css"],
        "js": ["build/static/js/main.59f78dd7.js"]
      }
    ]
  }